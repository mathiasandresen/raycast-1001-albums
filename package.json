{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "1001-albums",
  "title": "1001 Albums History",
  "description": "An extension for viewing your history of albums from 1001albumsgenerator.com",
  "icon": "extension_icon.png",
  "author": "mathiasandresen",
  "categories": [
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Show History",
      "description": "Show your history of albums as well as the current album",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "projectId",
      "title": "Project Id",
      "type": "textfield",
      "description": "The project id from 1001albumsgenerator.com.",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.65.0",
    "@raycast/utils": "^1.10.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
